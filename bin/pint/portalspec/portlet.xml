<portlet-app
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
	http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
	
	<portlet>
		<description>消息中心</description>
		<portlet-name>MsgCenterMenuPortlet</portlet-name>
		<display-name>消息中心</display-name>
		<portlet-class>
			nc.uap.portal.portlets.message.MessageCenterPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>117</value>
				<read-only>true</read-only>
			</preference>
			<preference>
				<name>if_height</name>
				<value>0</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_width</name>
				<value>100%</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_src</name>
				<description>链接地址</description>
				<value><![CDATA[/portal/core/center.jsp?pageId=center]]></value>
				<read-only>false</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>消息中心</title>
		</portlet-info>
		<supported-publishing-event>
			<name>switchIframeContentEvent</name>
		</supported-publishing-event>
	</portlet>
	
	<portlet>
		<description>消息中心弹出Portlet</description>
		<portlet-name>MsgCenterPopupPortlet</portlet-name>
		<display-name>消息中心</display-name>
		<portlet-class>
			nc.uap.portal.portlets.MsgCenterPopupPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>117</value>
				<read-only>true</read-only>
			</preference>
			<preference>
				<name>scrolling</name>
				<description>是否允许滚动</description>
				<value>yes</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>消息中心</title>
		</portlet-info>
	</portlet>
	
	<portlet>
		<description>任务中心Portlet</description>
		<portlet-name>TaskCenterPortlet</portlet-name>
		<display-name>任务中心Portlet</display-name>
		<portlet-class>
			nc.uap.portlet.iframe.MonitorIframePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>if_src</name>
				<description>链接地址</description>
				<value><![CDATA[/portal/app/mockapp/taskcenter?nodecode=E0010101]]></value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_height</name>
				<value>-1</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_width</name>
				<value>100%</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>工作中心Portlet</title>
		</portlet-info>
		<supported-processing-event>
			<name>switchIframeContentEvent</name>
		</supported-processing-event>
		
		<supported-processing-event>
			<name>doMaxEvent</name>
		</supported-processing-event>
	</portlet>
	
	<portlet>
		<description>我的功能Portlet</description>
		<portlet-name>MyFunctionPortlet</portlet-name>
		<display-name>我的功能</display-name>
		<portlet-class>
			nc.uap.portal.portlets.MyFunctionPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>117</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>我的功能</title>
		</portlet-info>
	</portlet>
	
	<portlet>
		<description>待办流程Portlet</description>
		<portlet-name>MyWflPortlet</portlet-name>
		<display-name>待办流程</display-name>
		<portlet-class>
			nc.uap.portal.portlets.MyWflPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>117</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>我的流程</title>
		</portlet-info>
	</portlet>
	
	<portlet>
		<description>集成Portlet</description>
		<portlet-name>WebApplicationPortlet</portlet-name>
		<display-name>集成Portlet</display-name>
		<portlet-class>
			nc.uap.portal.portlets.WebApplicationPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>system_code</name>
				<value>NC</value>
				<read-only>false</read-only>
				<description>系统编码</description>
			</preference>
			<preference>
				<name>version</name>
				<value>117</value>
				<read-only>true</read-only>
			</preference>
			<preference>
				<name>funcode</name>
				<value></value>
				<read-only>false</read-only>
				<description>功能节点编号</description>
			</preference>
			<preference>
				<name>if_height</name>
				<value>0</value>
				<read-only>false</read-only>
				<description>高度</description>
			</preference>
			<preference>
				<name>if_width</name>
				<value>100%</value>
				<read-only>false</read-only>
				<description>宽度</description>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>WebApplicationPortlet</title>
		</portlet-info>
		<supported-publishing-event>
			<name>switchIframeContentEvent</name>
		</supported-publishing-event>
	</portlet>
	
	<portlet>
		<description>表单集成Portlet</description>
		<portlet-name>FormIntegrationPortlet</portlet-name>
		<display-name>表单集成Portlet1</display-name>
		<portlet-class>
			nc.uap.portal.portlets.integration.IntegrationPortletWithForm
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<init-param>
			<name>system_code</name>
			<value>FormPortlet</value>
		</init-param>
		<init-param>
			<name>share_level</name>
			<value>1</value>
		</init-param>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>117</value>
				<read-only>true</read-only>
			</preference>
			<preference>
				<name>system_code</name>
				<value></value>
				<read-only>false</read-only>
				<description>系统编码</description>
			</preference>
			<preference>
				<name>gateUrl</name>
				<value></value>
				<read-only>false</read-only>
				<description>网关地址</description>
			</preference>
			<preference>
				<name>share_level</name>
				<value>1</value>
				<read-only>false</read-only>
				<description>共享级别</description>
			</preference>
			<preference>
				<name>if_height</name>
				<value>0</value>
				<read-only>false</read-only>
				<description>高度</description>
			</preference>
			<preference>
				<name>if_width</name>
				<value>100%</value>
				<read-only>false</read-only>
				<description>宽度</description>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>集成Portlet1</title>
		</portlet-info>
		<supported-publishing-event>
			<name>switchIframeContentEvent</name>
		</supported-publishing-event>
	</portlet>
	 
	
	<default-namespace>http://ufida.com.cn</default-namespace>
</portlet-app>