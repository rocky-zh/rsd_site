<portlet-app
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
	http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
   
   
	<portlet>
		<description>通用浮动挂接Portlet</description>
		<portlet-name>FloatLinkPortlet</portlet-name>
		<display-name>通用浮动挂接Portlet</display-name>
		<portlet-class>
			nc.uap.portal.portlets.linktree.FloatLinkTreePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>linkgroup</name>
				<value></value>
				<read-only>false</read-only>
				<description>菜单分类</description>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>浮动导航Portlet</title>
		</portlet-info>
		<supported-publishing-event>
			<name>switchIframeContentEvent</name>
		</supported-publishing-event>
		<supported-processing-event>
			<name>execScriptEvent</name>
		</supported-processing-event>
		<supported-publishing-event>
			<name>showNavEvent</name>
		</supported-publishing-event>
	</portlet>
   	<portlet>
		<description>导航条Portlet</description>
		<portlet-name>NavigationPortlet</portlet-name>
		<display-name>导航条Portlet</display-name>
		<portlet-class>
			nc.uap.portal.portlets.linktree.NavigationPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>导航条</title>
		</portlet-info>
		<supported-processing-event>
			<name>showNavEvent</name>
		</supported-processing-event>
	</portlet>
	<portlet>
		<description>版权Portlet</description>
		<portlet-name>CopyRightPortlet</portlet-name>
		<display-name>版权Portlet</display-name>
		<portlet-class>
			nc.uap.portal.portlets.banner.CopyrightPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>版权</title>
		</portlet-info>
	</portlet>
	<portlet>
		<description>LFWPortlet</description>
		<portlet-name>LFWPortlet</portlet-name>
		<display-name>LFWPortlet</display-name>
		<portlet-class>
			nc.uap.portal.portlets.message.MessageCenterPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>117</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>LFWPortlet</title>
		</portlet-info>
	</portlet>
	
	<portlet>
		<description>通用Outlook挂接Portlet</description>
		<portlet-name>OutLookLinkTreePortlet</portlet-name>
		<display-name>通用浮动挂接Portlet</display-name>
		<portlet-class>
			nc.uap.portal.portlets.linktree.FloatLinkTreePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>linkgroup</name>
				<value></value>
				<read-only>false</read-only>
				<description>菜单分类</description>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>浮通用Outlook挂接Portlet</title>
		</portlet-info>
		<supported-publishing-event>
			<name>switchIframeContentEvent</name>
		</supported-publishing-event>
		<supported-processing-event>
			<name>execScriptEvent</name>
		</supported-processing-event>
	</portlet>
	
	<event-definition>
		<name>showNavEvent</name>
		<value-type>java.lang.String</value-type>
	</event-definition>
	<default-namespace>http://ufida.com.cn</default-namespace>
</portlet-app>