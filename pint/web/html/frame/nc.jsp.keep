<%@ page contentType="text/html;charset=UTF-8" %>
<html>
<head>
<script>
function goLogin(){
	var url = '<%= request.getParameter("fwd") %>&height=' +  document.body.clientHeight +'&width=' + document.body.clientWidth;
	document.location.href = url; 
}
</script>
</head>
<body onload="goLogin()">
</body>
</html>
