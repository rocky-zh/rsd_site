<portlet-app
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
	http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
	<portlet>
		<description>集团管理导航</description>
		<portlet-name>GrpMgrPortlet</portlet-name>
		<display-name>集团管理导航</display-name>
		<portlet-class>
			nc.uap.portal.portlets.linktree.FloatLinkTreePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>linkgroup</name>
				<value>0000z010000000000002</value>
				<read-only>false</read-only>
				<description>菜单分类</description>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>集团管理导航Portlet</title>
		</portlet-info>
		<supported-publishing-event>
			<name>switchIframeContentEvent</name>
		</supported-publishing-event>
		<supported-processing-event>
			<name>execScriptEvent</name>
		</supported-processing-event>
	</portlet>
	
	<portlet>
		<description>管理内容</description>
		<portlet-name>MgrContentPortlet</portlet-name>
		<display-name>管理内容</display-name>
		<portlet-class>
			nc.uap.portlet.iframe.MonitorIframePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>if_src</name>
				<description>链接地址</description>
				<value><![CDATA[/portal/pages/view.jsp]]></value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_height</name>
				<value>-1</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_width</name>
				<value>100%</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>管理内容</title>
		</portlet-info>
		<supported-processing-event>
			<name>switchIframeContentEvent</name>
		</supported-processing-event>
		
		<supported-processing-event>
			<name>doMaxEvent</name>
		</supported-processing-event>
	</portlet>
	
	
	<portlet>
		<description>收藏夹</description>
		<portlet-name>MyFavPortlet</portlet-name>
		<display-name>收藏夹</display-name>
		<portlet-class>
			nc.uap.portal.portlets.message.MessageCenterPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>version</name>
				<value>117</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>收藏夹</title>
		</portlet-info>
	</portlet>
	
	 
	<portlet>
		<description>IPad子页面Portlet</description>
		<portlet-name>IPadPortlet</portlet-name>
		<display-name>IPad子页面Portlet</display-name>
		<portlet-class>
			nc.uap.portlet.iframe.IPadIframePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>if_src</name>
				<description>链接地址</description>
				<value><![CDATA[/portal/pages/view.jsp]]></value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_height</name>
				<value>560</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>if_width</name>
				<value>100%</value>
				<read-only>false</read-only>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>管理内容</title>
		</portlet-info>
		<supported-processing-event>
			<name>switchIframeContentEvent</name>
		</supported-processing-event>
	</portlet>
	<portlet>
		<description>BAP iPad Portlet</description>
		<portlet-name>BAPiPadPortlet</portlet-name>
		<display-name>BAP iPad Portlet</display-name>
		<portlet-class>
			nc.uap.portal.portlets.linktree.BAPiPadPortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-preferences>
			<preference>
				<name>linkgroup</name>
				<value>0000AA10000000000OOY</value>
				<read-only>false</read-only>
				<description>链接组编码</description>
			</preference>
			<preference>
				<name>version</name>
				<value>1</value>
				<read-only>true</read-only>
			</preference>
		</portlet-preferences>
		<portlet-info>
			<title>OutLook链接组Portlet</title>
		</portlet-info>
		<supported-publishing-event>
			<name>switchIframeContentEvent</name>
		</supported-publishing-event>
		<supported-processing-event>
			<name>execScriptEvent</name>
		</supported-processing-event>
	</portlet>
		 
	<default-namespace>http://ufida.com.cn</default-namespace>
	<event-definition>
		<name>AdminPortletClick</name>
		<value-type>java.lang.String</value-type>
	</event-definition>
	<event-definition>
		<name>switchIframeContentEvent</name>
		<value-type>java.lang.String</value-type>
	</event-definition>
	<event-definition>
		<name>doMaxEvent</name>
		<value-type>java.lang.String</value-type>
	</event-definition>
	<event-definition>
		<name>execScriptEvent</name>
		<value-type>java.lang.String</value-type>
	</event-definition>
 
</portlet-app>
